@startuml shoes
entity shoes {
  + ID [PK]
  --
  name
  image_url_xx
  maker
  accessories
  remark
  serial_number : VARCHAR(50)
  status : BOOLEAN
  status_code : CHAR(20)
  created_at : TIMESTAMP
  updated_at : TIMESTAMP
  --
  + user_id [FK]
  + storage_id [FK]
  + shoes_deposit_id [FK]
  + shoes_return_id [FK]
  + shoes_purchase_id [FK]
  + valuation_id [FK]
}
@enduml